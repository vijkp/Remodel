
DEFAULT <- all

all <- remodel_rm

remodel_rm <-  main.o, misc.o, file.o, maindefs.o, md5hash.o, threads.o, queue.o : "gcc main.o misc.o file.o maindefs.o md5hash.o threads.o queue.o -lssl -lcrypto -lpthread -o remodel_rm.new; mv -f remodel_rm.new remodel_rm"

clean <- : "rm -fv remodel_rm *.o .remodel/*"

main.o      <- main.c, main.h, misc.h, maindefs.h, threads.h : "gcc   -c main.c -o main.o"
misc.o      <- misc.c, misc.h, maindefs.h : "gcc   -c misc.c -o misc.o"
file.o      <- file.c, misc.h, file.h, maindefs.h :	"gcc   -c file.c -o file.o"
maindefs.o  <- maindefs.c, maindefs.h, misc.h : "gcc   -c maindefs.c -o maindefs.o"
md5hash.o   <- md5hash.c, maindefs.h, misc.h : "gcc   -lssl -lcrypto -c md5hash.c -o md5hash.o "
threads.o   <- threads.c, maindefs.h, threads.h : "gcc   -c threads.c -o threads.o "
queue.o     <- queue.c, queue.h, maindefs.h:"gcc   -c queue.c -o queue.o"
